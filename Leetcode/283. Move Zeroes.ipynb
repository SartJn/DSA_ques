    "n = len(nums)\n",
    "i = 0\n",
    "count = 0\n",
    "while count < n:\n",
    "    if nums[i] == 0:\n",
    "        nums.append(0)\n",
    "        nums.pop(i)\n",
    "    else:\n",
    "        i += 1\n",
    "    count += 1\n",
    "return nums\n"
