   "source": [
    "left = 0\n",
    "right = len(nums)-1\n",
    "mid = right//2\n",
    "curmin = minn = nums[mid]\n",
    "while left < right:\n",
    "    if nums[mid] < nums[right]:\n",
    "        right = mid\n",
    "    else:\n",
    "        left = mid+1\n",
    "    mid = left + (right-left) // 2\n",
    "    curmin = nums[mid]\n",
    "    minn = min(curmin, minn)\n",
    "    \n",
    "\n",
    "return minn\n",
    "\n"
   ]
  }
