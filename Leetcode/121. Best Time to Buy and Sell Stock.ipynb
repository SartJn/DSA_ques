   "source": [
    "prof = 0\n",
    "buy = prices[0]\n",
    "i = 1\n",
    "while i < len(prices):\n",
    "    if buy > prices[i]:\n",
    "        buy = prices[i]\n",
    "    prof = max(prof, prices[i]-buy)\n",
    "    i += 1\n",
    "\n",
    "return prof "
   ]
